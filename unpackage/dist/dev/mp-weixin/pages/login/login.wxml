<view class="main"><view class="login"><view class="login-logo"></view></view><view class="login-info"><form data-event-opts="{{[['submit',[['toLogin',['$event']]]]]}}" bindsubmit="__e"><label class="login-input"><view class="mail icon"></view><input type="text" placeholder="邮箱" data-event-opts="{{[['input',[['__set_model',['$0','email','$event',[]],['info']]]]]}}" value="{{info.email}}" bindinput="__e"/></label><label class="login-input"><view class="password icon"></view><input password="true" placeholder="密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['info']]]]]}}" value="{{info.password}}" bindinput="__e"/></label></form></view><view class="login-btn btn"><button form-type="submit" type="primary" data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" bindtap="__e">登录</button></view><view class="login-signup"><button class="btn" type="default"><navigator url="/pages/regist/regist">注册账号</navigator></button></view></view>