<view class="weekly"><form data-event-opts="{{[['submit',[['submit',['$event']]]],['reset',[['',['$event']]]]]}}" bindsubmit="__e" bindreset="__e"><view class="header"><text class="current-week">{{"第"+week+"周"}}</text><text class="user-info">{{user.groupName+" :"+user.userName}}</text></view><view class="content"><view class="input-area"><text class="input-title">本周完成</text><input class="input-content" placeholder="必填" data-event-opts="{{[['input',[['__set_model',['$0','complete','$event',[]],['weekly']]]]]}}" value="{{weekly.complete}}" bindinput="__e"/></view><view class="input-area"><text class="input-title">所遇问题</text><input class="input-content" placeholder="必填" data-event-opts="{{[['input',[['__set_model',['$0','trouble','$event',[]],['weekly']]]]]}}" value="{{weekly.trouble}}" bindinput="__e"/></view><view class="input-area"><text class="input-title">下周计划</text><input class="input-content" placeholder="必填" data-event-opts="{{[['input',[['__set_model',['$0','plane','$event',[]],['weekly']]]]]}}" value="{{weekly.plane}}" bindinput="__e"/></view><view class="input-area"><text class="input-title">作品链接</text><input class="input-content" placeholder="GitHub、站酷等，选填" data-event-opts="{{[['input',[['__set_model',['$0','url','$event',[]],['weekly']]]]]}}" value="{{weekly.url}}" bindinput="__e"/></view></view><view class="btn-block"><button class="uni-btn" form-type="submit" type="primary">提交</button></view></form></view>